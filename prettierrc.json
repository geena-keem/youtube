{
	"trailingComma": "es5",
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"useTabs": true,
	"printWidth": 120,
	"endOfLine": "lf",
	"overrides": [
		{
			"files": "**/*.yaml",
			"options": {
				"tabWidth": 2,
				"useTabs": false
			}
		}
	]
}